[sub_resource type="GDScript" id="GDScript_ovp2j"]
resource_name = "Login"
script/source = "@tool
extends Control

var plg_config: ConfigFile = ConfigFile.new()

var email: String
var password: String

func _ready():
	plg_config.load(\"res://addons/pogr_plugin/plugin.cfg\")
	visible = plg_config.get_value(\"plugin\",\"logged_out\",true)


func _on_email_text_changed(new_text):
	email = new_text
	$Panel/MarginContainer/VBoxContainer/Login.disabled = !(email.length() > 4 && email.contains(\"@\") && password.length() > 7)

func _on_password_text_changed(new_text):
	password = new_text
	$Panel/MarginContainer/VBoxContainer/Login.disabled = !(email.length() > 4 && email.contains(\"@\") && password.length() > 7)


func _on_login_pressed():
	$AnimationPlayer.play(\"fade_out\")
	await get_tree().create_timer(0.3).timeout
	plg_config.set_value(\"plugin\",\"logged_out\",false)
	plg_config.save(\"res://addons/pogr_plugin/plugin.cfg\")
	visible = false
"

