[sub_resource type="GDScript" id="GDScript_r7bbs"]
resource_name = "Setup"
script/source = "@tool
extends Control

var client_id: String
var build_id: String

var config: ConfigFile = ConfigFile.new()

func _ready():
	config.load(\"res://addons/pogr_plugin/save/config.pogr\")
	visible = !(config.has_section_key(\"api\",\"client_id\") && config.has_section_key(\"api\",\"build_id\"))



func _on_build_id_text_changed(new_text):
	build_id = new_text
	$PanelContainer/MarginContainer/VBoxContainer/StartButton/Start.disabled = !(client_id.length() > 0 && build_id.length() > 0)

func _on_client_id_text_changed(new_text):
	client_id = new_text
	$PanelContainer/MarginContainer/VBoxContainer/StartButton/Start.disabled = !(client_id.length() > 0 && build_id.length() > 0)

func _on_start_pressed():
	config.set_value(\"api\",\"client_id\",client_id)
	config.set_value(\"api\",\"build_id\",build_id)
	config.save(\"res://addons/pogr_plugin/save/config.pogr\")
	$AnimationPlayer.play(\"fade_out\")
	await $AnimationPlayer.animation_finished
	visible = false

func _on_import_button_pressed():
	print(\"Wait for native file mport in GD 4.2\")

func _on_text_submitted(_new_text):
	if(client_id.length() > 0 && build_id.length() > 0):
		_on_start_pressed()
"

